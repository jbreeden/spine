{
  "name": "Spine",
  "description": "Debugging utilities for Backbone applications.",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["spine.js", "sinon.js"],
  "manifest_version": 2
}
